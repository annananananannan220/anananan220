-- List of URLs for your Lua files
local urls = {
    "https://raw.githubusercontent.com/your-username/your-repo-name/main/gui.lua",
    "https://raw.githubusercontent.com/your-username/your-repo-name/main/wh.lua",
    "https://raw.githubusercontent.com/your-username/your-repo-name/main/cs.lua",
    "https://raw.githubusercontent.com/your-username/your-repo-name/main/set.lua",
    "https://raw.githubusercontent.com/your-username/your-repo-name/main/otr.lua"
}

-- Function to load and execute a Lua file
local function loadAndExecute(url)
    local scriptContent = game:HttpGet(url)
    loadstring(scriptContent)()
end

-- Run all scripts concurrently using coroutines
for _, url in ipairs(urls) do
    coroutine.wrap(loadAndExecute)(url)
end
